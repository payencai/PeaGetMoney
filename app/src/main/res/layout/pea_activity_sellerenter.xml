<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:background="@color/white"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context=".home.activity.SellerenterActivity">
    <include layout="@layout/pea_top"></include>
    <TextView
        android:id="@+id/nolicense"
        android:layout_marginTop="50dp"
        android:textSize="16sp"
        android:textColor="@color/tv_999"
        android:text="无证商户"
        android:gravity="center"
        android:layout_marginRight="30dp"
        android:layout_marginLeft="30dp"
        android:background="@drawable/shape_solid_999"
        android:layout_width="match_parent"
        android:layout_height="50dp"/>
    <TextView
        android:id="@+id/selfsell"
        android:layout_marginTop="30dp"
        android:textSize="16sp"
        android:textColor="@color/tv_999"
        android:text="个体商户"
        android:gravity="center"
        android:layout_marginRight="30dp"
        android:layout_marginLeft="30dp"
        android:background="@drawable/shape_solid_999"
        android:layout_width="match_parent"
        android:layout_height="50dp"/>
    <TextView
        android:id="@+id/enterprise"
        android:layout_marginTop="30dp"
        android:textSize="16sp"
        android:textColor="@color/tv_999"
        android:text="企业商户"
        android:gravity="center"
        android:layout_marginRight="30dp"
        android:layout_marginLeft="30dp"
        android:background="@drawable/shape_solid_999"
        android:layout_width="match_parent"
        android:layout_height="50dp"/>
    <TextView
        android:layout_marginTop="30dp"
        android:textSize="14sp"
        android:textColor="@color/tv_999"
        android:text="请选择符合自身条件的选项"
        android:gravity="center"
        android:layout_marginRight="30dp"
        android:layout_marginLeft="30dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</LinearLayout>